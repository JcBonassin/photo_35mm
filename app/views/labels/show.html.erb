<div id="preloder">
	<div class="loader"></div>
</div>

<div class="gallery__page">
    <div class= "center-this">
       <h2>Photos that contain <%= @label.tag %></h2>
    </div>
  <div class="gallery__warp"> 
    <div class="row">
       <div class="column">
	   <% @matches.each do |photo| %>
         <%= link_to photo do %>
            <%= image_tag photo.image_url, class: "gallery__item fresco", data_fresco_group: "gallery" %>
                <small><%= link_to "By #{photo.user.slug}", photo.user %></small>
            <% end %>     
	     <% end %>
       </div>
    </div>
   </div>
</div>
 

